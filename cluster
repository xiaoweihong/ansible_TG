# 引擎主节点ip (引擎集群的主节点ip)
[bigtoe]
192.168.2.159

# kafka集群ip (如果有规划kafka集群的机器，此处填写实际kafka集群的ip)
# 如无规划kafka集群，此处填写引擎主节点ip
# 15.234.50 [20:24]此处是简写，表示为15.234.50.20, 15.234.50.21, 15.234.50.22…
# 也可以单独写全的ip，下面同理
[kafka]
192.168.2.159



#  kafka集群每台机器的端口号
# 如果是用引擎的kafka，此处填写为32400
[all:vars]
port=32400

# kafka集群ip (机器数量推荐3台，5台或者7台，数量不宜过多)
[kafka_servers]
15.234.50.20 kafka_port=9092  id=1
15.234.50.21 kafka_port=9092  id=2
15.234.50.22 kafka_port=9092  id=3
15.234.50.23 kafka_port=9092  id=4
15.234.50.24 kafka_port=9092  id=5

